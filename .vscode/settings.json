{
    "liveServer.settings.fullReload": true,
    "liveServer.settings.ignoreFiles":[
        ".vscode/**",
        "**/*.scss",
        "**/*.sass",
        "**/*.ts",
        "packages/**",
        "utils/**",
        "node_modules/**",
        "assets/**",
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/*.js": true
    }
}
